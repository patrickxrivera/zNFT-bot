generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pgmigrations {
  id     Int      @id @default(autoincrement())
  name   String
  run_on DateTime
}

model Block {
  id          Int       @id @default(autoincrement())
  number      Int       @unique
  processedAt DateTime?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt

  @@index([number], name: "blocks_number_index")
  @@map("blocks")
}
